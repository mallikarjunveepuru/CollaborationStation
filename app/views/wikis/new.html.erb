<h1>New Wiki</h1>
<div class="row">
    <div class="col-md-8">
      <%= form_for @wiki do |f| %>
      <div class="form-group">
         <%= f.label :title %>
         <%= f.text_field :title, class: 'form-control', placeholder: "Enter wiki name" %>
       </div>
       <div class="form-group">
         <%= f.label :body %>
         <%= f.text_area :body, class: 'form-control', placeholder: "Enter Information" %>
       </div>
      
       <%= f.submit "Save", class: 'btn btn-info' %>
       <% end %>
     </div>
   </div>


