<h1>Wikis</h1>
        <% if policy(Wiki.new).create? %>
        <%= link_to "New Wiki", new_wiki_path, class: "btn btn-info" %>
      <% end %>
      <br>
<div class="row">
  <div class="col-md-8">
    <% @wikis.each do |wiki| %>
      <div class="media">
        <div class="media-body">
          <h4 class="media-heading">
            <%= link_to wiki.title, wiki %>
          </h4>
        </div>
      </div>
      <% end %>
      
      <%= will_paginate @wikis %>
